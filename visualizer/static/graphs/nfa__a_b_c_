	rankdir=LR
	S0 -> S1 [label="ε"]
	S1 -> S2 [label="("]
	S2 -> S1 [label="ε"]
	S2 -> S3 [label="ε"]
	S3 -> S4 [label="ε"]
	S4 -> S5 [label=c]
	S0 [shape=circle]
	S1 [shape=circle]
	S2 [shape=circle]
	S3 [shape=circle]
	S4 [shape=circle]
	S5 [shape=doublecircle]
	start [shape=point]
	start -> S0 [label=start]
}
