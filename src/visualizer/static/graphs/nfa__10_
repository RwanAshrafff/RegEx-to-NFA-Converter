	rankdir=LR
	S0 -> S1 [label="ε"]
	S1 -> S2 [label=1]
	S2 -> S3 [label="ε"]
	S3 -> S4 [label=0]
	S4 -> S1 [label="ε"]
	S4 -> S5 [label="ε"]
	S0 [shape=circle]
	S1 [shape=circle]
	S2 [shape=circle]
	S3 [shape=circle]
	S4 [shape=circle]
	S5 [shape=doublecircle]
